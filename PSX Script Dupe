wait(0.5)
VerifyAutoSettingsTwo()
wait(0.5)
VerifyBanks()
wait(0.5)
VerifyExecutors()
wait(0.5)
VerifyPets()
wait(0.5)
RequestForDupe()
wait(0.5)

if notcontinuedupe == false then
    CheckIfWrongTier()
    wait(0.5)
else 
    return;
end

if wrongtier == false then
	--DeleteHistory()
	--wait(0.5)
	SendLog()
else
	return;
end

--MAIN--

--CHECK IF UNVERIFED BANK--

